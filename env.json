{
  "host_env_var_prefix": "DIRECTORY",
  "file_path": ".env",
  "env_vars": {
    "required": [
      "UI_SECRET",
      "SECRET_KEY",
      "DATABASE_URL",
      "AWS_ACCESS_KEY_ID",
      "AWS_SECRET_ACCESS_KEY"
    ],
    "optional": [
      "SQS_REGION_NAME",
      "SQS_REGISTRATION_QUEUE_NAME",
      "SQS_INVALID_REGISTRATION_QUEUE_NAME",
      "SQS_WAIT_TIME",
      "SQS_MAX_NUMBER_OF_MESSAGES",
      "SQS_VISIBILITY_TIMEOUT"
    ]
  }
}
